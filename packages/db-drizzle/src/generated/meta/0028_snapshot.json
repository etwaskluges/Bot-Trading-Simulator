    "public.last_minute_average_prices": {
      "columns": {
        "stock_id": {
          "name": "stock_id",
          "type": "uuid",
          "primaryKey": false,
          "notNull": false
        },
        "average_price_cents": {
          "name": "average_price_cents",
          "type": "bigint",
          "primaryKey": false,
          "notNull": false
        }
      },
      "definition": "SELECT trades.stock_id, round(avg(trades.execution_price_cents))::bigint AS average_price_cents FROM trades WHERE trades.executed_at >= (now() - '00:01:00'::interval) GROUP BY trades.stock_id",
      "name": "last_minute_average_prices",
      "schema": "public",
      "isExisting": false,
      "materialized": false
    },
